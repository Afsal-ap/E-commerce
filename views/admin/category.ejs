        <div class="card">
            <div class="card-body">
                <div class="row">
                    
                    <div class="col-md-12">
                        <div class="table-responsive">
                            <table class="table table-hover">
                                <thead>
                                    <tr>
                                        
                                        <th>NO</th>
                                        <th>Name</th>
                                        <th>Description</th>
                                        <th>Edit</th>     
                                        <th>Action</th>
                                    </tr>
                                </thead>
                                <tbody>
                                    <% if(category) { %>
                                    <% for(let i=0; i < category.length; i++) { %>
                                        <tr>
                                            
                                            <td> <%= i+1 %></td>
                                            
                                            
                                            <td> <%= category[i].name %></td>
                                           
                                            
                                            <td> <%= category[i].description %></td>
                                           
                                            <td> 
                                                <a href="/admin/editCategory?id=<%=category[i]._id%>"><span
                                                       class="badge bg-light">EDIT</span></a>
                                                    </td>
                                                   <td> 
                                                   <% if(category[i].is_blocked) { %>
                                                    <button class="badge bg-danger" style="border : none;"
                                    onclick="confirmList('<%=category[i]._id%>')">list</button>
                                      <%  } else { %> 
                                          <button class="badge badge-soft-success" style="border: none;"
                                          onclick="confirmUnlist('<%=category[i]._id%>')">unlist</button>       
                                               <% } %>
                                                   </td>

                                        </tr>

                                 <%   } %>
                                  <% } %>

                        <!-- dropdown //end -->
                                        </td>
                                    </tr>
                                </tbody>
                            </table>
                       </div>
                    </div>
                    <!-- .col// -->
                </div>
                <!-- .row // -->
               
            </div>
