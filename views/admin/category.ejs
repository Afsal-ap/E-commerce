            <script>

                function confirmList(userId) { 
                      Swal.fire({
                            title: "Are you sure?",
                            text: "You want to list this product?",
                             icon: "warning",
                            showCancelButton: true,
                            confirmButtonColor: "#3085d6",
                            cancelButtonColor: "#d33",
                            confirmButtonText: "Yes, list it!"
                            }).then((result) => {
                               if (result.isConfirmed) {
                                      unlist(userId);
                              }
                            });
                           }  


function confirmUnlist(userId) {
        Swal.fire({
            title: "Are you sure?",
            text: "You want to unlist this product?",
            icon: "warning",
            showCancelButton: true,
            confirmButtonColor: "#3085d6",
            cancelButtonColor: "#d33",
            confirmButtonText: "Yes, unlist it!"
        }).then((result) => {
            if (result.isConfirmed) {
                list(userId);
            }
        });
    }


                                  function unlist(id) {
                                    console.log("yy");
                                 
                                  fetch(`/admin/unlistCategory/${id}`, { method: 'POST' })
                                    .then(response => response.json())
                                    .then(data => {
                                      if (data.unlist===true) {
                                       location.reload() //       
                                       
                                      } else{
                                        console.error(data.error)
                                      }
                                    })
                                    .catch(error => console.error(error));
                                }
                              
                                function list(id) {
                                    console.log("yayay");
                                 
                                  fetch(`/admin/listCategory/${id}`, { method: 'POST' })
                                    .then(response => response.json())
                                    .then(data => {
                                      if (data.list ===  true) {
                                       location.reload() 
                                       
                                      }else{
                                        console.error(data.message);
                                      }
                                    })
                                    .catch(error => console.error(error));
                                }
                              
    </script>

