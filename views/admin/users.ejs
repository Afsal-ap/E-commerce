                            <script>
                                

                                function confirmBlock(userId) { console.log("oooo");
    Swal.fire({
        title: "Are you sure?",
        text: "You want to block this user?",
        icon: "warning",
        showCancelButton: true,
        confirmButtonColor: "#3085d6",
        cancelButtonColor: "#d33",
        confirmButtonText: "Yes, block it!"
    }).then((result) => {
        if (result.isConfirmed) {
            block(userId);
        }
    });
}


function confirmUnblock(userId) {
        Swal.fire({
            title: "Are you sure?",
            text: "You want to unblock this user?",
            icon: "warning",
            showCancelButton: true,
            confirmButtonColor: "#3085d6",
            cancelButtonColor: "#d33",
            confirmButtonText: "Yes, unblock it!"
        }).then((result) => {
            if (result.isConfirmed) {
                unblock(userId);
            }
        });
    }

                            

                                function unblock(userId) {
                                    console.log("yy");
                                  // Use AJAX to send a POST request to the server to toggle block status
                                  fetch(`/admin/unblockusers/${userId}`, { method: 'POST' })
                                    .then(response => response.json())
                                    .then(data => {
                                      if (data.unblock===true) {
                                       location.reload() //       
                                       
                                      } else{
                                        console.error(data.error)
                                      }
                                    })
                                    .catch(error => console.error(error));
                                }
                              
                                function block(userId) {
                                    console.log("yayay");
                                  // Use AJAX to send a POST request to the server to toggle block status
                                  fetch(`/admin/blockusers/${userId}`, { method: 'POST' })
                                    .then(response => response.json())
                                    .then(data => {
                                      if (data.block ===  true) {
                                       location.reload() //         } else {
                                       
                                      }else{
                                        console.error(data.message);
                                      }
                                    })
                                    .catch(error => console.error(error));
                                }
                              
